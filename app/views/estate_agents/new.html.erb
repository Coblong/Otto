<% provide(:title, 'New Estate Agent') %>
<h1>New Estate Agent</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@estate_agent) do |f| %>
      <%= render 'shared/estate_agent_error_messages' %>
      
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :comment %>
      <%= f.text_field :comment %>

      <%= f.submit "Create estate agent", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>